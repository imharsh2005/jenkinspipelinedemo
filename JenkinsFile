pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Build') {
            steps {
                echo 'Build ....'
            }
        }
        stage('Deploy') {
            steps {
                echo 'deplying ...'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing ....'
            }
        }
        stage('Release') {
            steps {
                echo 'Released'
            }
        }
    }
}
